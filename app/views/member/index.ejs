<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
        <h1>Index Page</h1>
        <% for (var i=0; i < members.length; i++){ %>
            <a href="/members/<%= members[i].id; %>">
                <%= members[i].name %>
            </a>
            <%= members[i].nationality %>
            <%= members[i].dateOfBirth %>
            <%= members[i].nationalID %>
            <%= members[i].bloodType %><br>
        <% } %>
    </body>
</html>